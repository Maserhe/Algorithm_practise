#include<iostream>

using namespace std;
//筛选素数
const int N = 1e5;
int prime[N] ，cnt;
bool st[N];

void get_prime(int n){
    for(int i = 2; i < n / i; i ++ ){
        if(!st[i]){

            prime[cnt ++ ] = n;
            for( int j = i  + i  ; j <= n ; j += i ){
                st[j ]  =  true;
            }
        }

    }

}
int main(){
    int n;
    cin>>n;
    get_prime(n)；

    cnt<<cnt<<endl;


    return 0;
}